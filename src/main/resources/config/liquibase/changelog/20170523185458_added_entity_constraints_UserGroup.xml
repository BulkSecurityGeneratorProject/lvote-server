<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity UserGroup.
    -->
    <changeSet id="20170523185458-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="permited_group_id"
                                 baseTableName="user_group"
                                 constraintName="fk_user_group_permited_group_id"
                                 referencedColumnNames="id"
                                 referencedTableName="voting"/>

        <addForeignKeyConstraint baseColumnNames="owner_id"
                                 baseTableName="user_group"
                                 constraintName="fk_user_group_owner_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
